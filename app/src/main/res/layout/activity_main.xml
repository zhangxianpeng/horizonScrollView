<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/update_image_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/qb_px_83"
        android:layout_marginTop="@dimen/qb_px_112"
        app:srcCompat="@drawable/base_ic_back" />

    <TextView
        android:id="@+id/text_about_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:textSize="15sp"
        android:layout_marginTop="@dimen/qb_px_103"
        android:text="@string/app_bar" />

    <include layout="@layout/base_update_item" />
</RelativeLayout>